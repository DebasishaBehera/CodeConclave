{
  "version": 2,
  "builds": [
    {
      "src": "CodeConclave/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://codeconclaveserver.onrender.com/api/:path*"
    },
    {
      "source": "/:path*",
      "destination": "/CodeConclave/index.html"
    }
  ]
}