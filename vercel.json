{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist",
          "chunkSizeWarningLimit": 2000
        }
      }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "https://codeconclaveserver.onrender.com/api/$1",
            "headers": {
              "Access-Control-Allow-Origin": "*"
            }
          },
        {
        "src": "/_next/static/(.*)",
        "dest": "/dist/_next/static/$1"
        },
        {
            "src": "/(.*\\.(ico|svg|png|jpg|jpeg|css|js|json))",
            "dest": "/dist/$1"
        }
    ]
    }
        